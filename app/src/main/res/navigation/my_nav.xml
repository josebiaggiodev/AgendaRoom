<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/my_nav"
    app:startDestination="@id/listaContatosFragment">

    <fragment
        android:id="@+id/listaContatosFragment"
        android:name="br.edu.ifsp.aluno.agendaroom.ListaContatosFragment"
        android:label="@string/contatos"
        tools:layout="@layout/fragment_lista_contatos" >
        <action
            android:id="@+id/action_listaContatosFragment_to_cadastroFragment"
            app:destination="@id/cadastroFragment" />
    </fragment>
    <fragment
        android:id="@+id/cadastroFragment"
        android:name="br.edu.ifsp.aluno.agendaroom.CadastroFragment"
        android:label="@string/novo_contato"
        tools:layout="@layout/fragment_cadastro" />
</navigation>